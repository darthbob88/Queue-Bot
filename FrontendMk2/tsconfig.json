{
	"compilerOptions": {
		"alwaysStrict": true,
		"jsx": "react",
		"noImplicitAny": false,
		"noEmitOnError": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "es5",
		"module": "commonjs"
	},
	"compileOnSave": true,
	"include": [ "scripts/**/*" ],
	"exclude": [
		"node_modules",
		"wwwroot"
	]
}
